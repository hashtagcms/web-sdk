{
  "name": "@hashtagcms/web-sdk",
  "version": "1.0.3",
  "description": "Core JavaScript SDK for HashtagCMS - Form validation, analytics, and utilities",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "src",
    "docs",
    "index.d.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "hashtagcms",
    "form-validation",
    "analytics",
    "cms",
    "javascript-sdk",
    "form-handler",
    "newsletter",
    "form-submitter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hashtagcms/web-sdk.git"
  },
  "homepage": "https://github.com/hashtagcms/web-sdk#readme",
  "bugs": {
    "url": "https://github.com/hashtagcms/web-sdk/issues"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "webpack --mode development --watch --progress --color",
    "build": "webpack --mode production --progress --color",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "author": "HashtagCMS",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.8.1",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
